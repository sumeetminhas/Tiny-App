<!DOCTYPE html>

<html lang="en">

<head>

  <title>TinyApp</title>

</head>

<body>

<% include partials/_header %>

<ul>
  <% for (var shortURL in urls) { %>
    <li>
      <%= shortURL %> :
        <a href="/urls/<%= shortURL %>"> <%= urls[shortURL] %></a>
          <form method="POST" action="/urls/<%= shortURL %>/delete">
        <a href="/urls/<%= shortURL %>">Edit</a>
        <p>
          <button>Delete URL</button>
        </p>
      </form>
    </li>
  <% } %>
</ul>
</body>

</html>
